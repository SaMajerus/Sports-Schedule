@{
  Layout = "_Layout";
}

@using Schedule.Models;
@model List<Schedule.Models.Semester>;

<h1>Semesters</h1>

<ul>
  @foreach(Semester semester in Model)
  {
    <li>@Html.ActionLink($"{semester.Term}", "Details", new { id = semester.SemesterId })</li>
  }
</ul>

<p>@Html.ActionLink("Add new semester", "Create")</p>
<p>@Html.ActionLink("Home", "Index", "Home")</p>